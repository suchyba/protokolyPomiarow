<Window x:Class="ProtokolyPomiarow.Windows.ObjectsListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProtokolyPomiarow.Windows"
        mc:Ignorable="d"
        Title="Lista elementów" Height="400" Width="321.6"
        ResizeMode="NoResize"
        Closing="Window_Closing" WindowStartupLocation="CenterScreen"
        Icon="logo.png">
    <Grid>
        <ListBox x:Name="ListBox" Height="314" Margin="10,10,10.2,0" VerticalAlignment="Top" SelectionChanged="ListBox_SelectionChanged"/>
        <Button x:Name="AddButton" Content="Dodaj" HorizontalAlignment="Left" Margin="10,329,0,0" VerticalAlignment="Top" Width="75" Click="AddButton_Click"/>
        <Button x:Name="EditButton" Content="Edytuj/Usuń" Margin="110,329,110.2,0" VerticalAlignment="Top" Click="EditButton_Click" IsEnabled="False"/>
        <Button x:Name="SelectButton" Content="Wybierz" Margin="230,329,10.2,0" VerticalAlignment="Top" Click="SelectButton_Click" IsDefault="True" IsEnabled="False"/>

    </Grid>
</Window>
